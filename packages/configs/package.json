{
	"$schema": "https://www.schemastore.org/package.json",
	"author": {
		"email": "hunarroopcodes@gmail.com",
		"name": "Hunar Roop Kahlon"
	},
	"bugs": {
		"url": "https://github.com/sytizen/sytizen/issues"
	},
	"description": "Opinionated configuration files for @sytizen/* packages",
	"devDependencies": {
		"@biomejs/biome": "catalog:"
	},
	"exports": {
		"./biome.json": "./configs/biome.json",
		"./package.json": "./package.json",
		"./tsconfig.json": "./configs/tsconfig.json"
	},
	"files": [
		"configs"
	],
	"keywords": [
		"biome",
		"configs",
		"typescript"
	],
	"license": "MIT",
	"name": "@sytizen/configs",
	"packageManager": "bun@1.3.0",
	"repository": {
		"directory": "packages/configs",
		"type": "git",
		"url": "git+https://github.com/sytizen/sytizen.git"
	},
	"scripts": {
		"format": "biome check --write",
		"lint": "biome check"
	},
	"sideEffects": false,
	"type": "module",
	"version": "0.0.0"
}
